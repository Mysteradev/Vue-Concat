{
  "name": "@mystera/vue-concat",
  "version": "0.1.0",
  "description": "A simple Vue component to help you concat videos.",
  "main": "dist/vue-concat.ssr.js",
  "module": "dist/vue-concat.esm.js",
  "unpkg": "dist/vue-concat.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "author": "Kayoshi-dev",
  "license": "MIT",
  "keywords": [
    "vue",
    "concat",
    "videos",
    "mystera",
    "Kayoshi-dev"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mystera/vue-concat"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "rollup": "^2.19.0",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-vue": "^5.1.9",
    "cross-env": "^7.0.2",
    "minimist": "^1.2.5",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11",
    "core-js": "^3.6.5"
  }
}
